<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>io.swagger</groupId>
  <artifactId>AnsysPython-swagger-codegen</artifactId>
  <version>5.1.0</version>
  <packaging>jar</packaging>
  <name>AnsysPython-swagger-codegen</name>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <maven-plugin-version>1.0.0</maven-plugin-version>
    <base-package-version>0.1.0</base-package-version>
    <language>com.ansys.granta.codegen.AnsysPythonClientCodegen</language>
    <swagger-template-version>5.1.0-SNAPSHOT</swagger-template-version>
    <package-name>ansys-grantami-serverapi-openapi</package-name>
    <package-import-path>ansys.grantami.serverapi_openapi</package-import-path>
    <package-description>Autogenerated client library for the Granta MI Server API.</package-description>
    <homepage-url>https://github.com/ansys/grantami-serverapi-openapi</homepage-url>
    <documentation-url>https://grantami.docs.pyansys.com</documentation-url>
    <is-namespace-package>True</is-namespace-package>
    <v24r1-filename>server-api-24r1.json</v24r1-filename>
    <v24r2-filename>server-api-24r2.json</v24r2-filename>
    <v25r1-filename>server-api-25r1.json</v25r1-filename>
    <v25r2-filename>server-api-25r2.json</v25r2-filename>
    <dev-filename>server-api.json</dev-filename>
  </properties>

  <build>
    <plugins>
      <plugin>
        <groupId>io.swagger.codegen.v3</groupId>
        <artifactId>swagger-codegen-maven-plugin</artifactId>
        <version>3.0.74</version>
        <executions>
          <execution>
            <id>${v24r1-filename}</id>
            <goals>
              <goal>generate</goal>
            </goals>
            <configuration>
              <inputSpec>${project.basedir}/yaml/${v24r1-filename}</inputSpec>
              <language>${language}</language>
              <output>${package-name}</output>
              <generateApiTests>false</generateApiTests>
              <generateModelTests>false</generateModelTests>
              <configOptions>
                <packageName>${package-import-path}</packageName>
                <projectName>${package-name}</projectName>
                <projectDescription>${package-description}</projectDescription>
                <homepageUrl>${homepage-url}</homepageUrl>
                <documentationUrl>${documentation-url}</documentationUrl>
                <packageVersion>${project.version}</packageVersion>
                <buildNumber>${build-id}</buildNumber>
                <isReleaseBuild>${is-release}</isReleaseBuild>
                <isNamespacePackage>${is-namespace-package}</isNamespacePackage>
                <bindingNamespace>v2024r1</bindingNamespace>
              </configOptions>
            </configuration>
          </execution>
          <execution>
            <id>${v24r2-filename}</id>
            <goals>
              <goal>generate</goal>
            </goals>
            <configuration>
              <inputSpec>${project.basedir}/yaml/${v24r2-filename}</inputSpec>
              <language>${language}</language>
              <output>${package-name}</output>
              <generateApiTests>false</generateApiTests>
              <generateModelTests>false</generateModelTests>
              <configOptions>
                <packageName>${package-import-path}</packageName>
                <projectName>${package-name}</projectName>
                <projectDescription>${package-description}</projectDescription>
                <homepageUrl>${homepage-url}</homepageUrl>
                <documentationUrl>${documentation-url}</documentationUrl>
                <packageVersion>${project.version}</packageVersion>
                <buildNumber>${build-id}</buildNumber>
                <isReleaseBuild>${is-release}</isReleaseBuild>
                <isNamespacePackage>${is-namespace-package}</isNamespacePackage>
                <bindingNamespace>v2024r2</bindingNamespace>
              </configOptions>
            </configuration>
          </execution>
          <execution>
            <id>${v25r1-filename}</id>
            <goals>
              <goal>generate</goal>
            </goals>
            <configuration>
              <inputSpec>${project.basedir}/yaml/${v25r1-filename}</inputSpec>
              <language>${language}</language>
              <output>${package-name}</output>
              <generateApiTests>false</generateApiTests>
              <generateModelTests>false</generateModelTests>
              <configOptions>
                <packageName>${package-import-path}</packageName>
                <projectName>${package-name}</projectName>
                <projectDescription>${package-description}</projectDescription>
                <homepageUrl>${homepage-url}</homepageUrl>
                <documentationUrl>${documentation-url}</documentationUrl>
                <packageVersion>${project.version}</packageVersion>
                <buildNumber>${build-id}</buildNumber>
                <isReleaseBuild>${is-release}</isReleaseBuild>
                <isNamespacePackage>${is-namespace-package}</isNamespacePackage>
                <bindingNamespace>v2025r1</bindingNamespace>
              </configOptions>
            </configuration>
          </execution>
          <execution>
            <id>${v25r2-filename}</id>
            <goals>
              <goal>generate</goal>
            </goals>
            <configuration>
              <inputSpec>${project.basedir}/yaml/${v25r2-filename}</inputSpec>
              <language>${language}</language>
              <output>${package-name}</output>
              <generateApiTests>false</generateApiTests>
              <generateModelTests>false</generateModelTests>
              <configOptions>
                <packageName>${package-import-path}</packageName>
                <projectName>${package-name}</projectName>
                <projectDescription>${package-description}</projectDescription>
                <homepageUrl>${homepage-url}</homepageUrl>
                <documentationUrl>${documentation-url}</documentationUrl>
                <packageVersion>${project.version}</packageVersion>
                <buildNumber>${build-id}</buildNumber>
                <isReleaseBuild>${is-release}</isReleaseBuild>
                <isNamespacePackage>${is-namespace-package}</isNamespacePackage>
                <bindingNamespace>v2025r2</bindingNamespace>
              </configOptions>
            </configuration>
          </execution>
          <execution>
            <id>${dev-filename}</id>
            <goals>
              <goal>generate</goal>
            </goals>
            <configuration>
              <inputSpec>${project.basedir}/yaml/${dev-filename}</inputSpec>
              <language>${language}</language>
              <output>${package-name}</output>
              <generateApiTests>false</generateApiTests>
              <generateModelTests>false</generateModelTests>
              <configOptions>
                <packageName>${package-import-path}</packageName>
                <projectName>${package-name}</projectName>
                <projectDescription>${package-description}</projectDescription>
                <homepageUrl>${homepage-url}</homepageUrl>
                <documentationUrl>${documentation-url}</documentationUrl>
                <packageVersion>${project.version}</packageVersion>
                <buildNumber>${build-id}</buildNumber>
                <isReleaseBuild>${is-release}</isReleaseBuild>
                <isNamespacePackage>${is-namespace-package}</isNamespacePackage>
                <bindingNamespace>v2026r1</bindingNamespace>
              </configOptions>
            </configuration>
          </execution>
        </executions>
        <dependencies>
          <dependency>
            <groupId>com.ansys</groupId>
            <artifactId>pyansys-swagger-codegen</artifactId>
            <version>${swagger-template-version}</version>
            <scope>compile</scope>
          </dependency>
        </dependencies>
      </plugin>
    </plugins>
  </build>

  <profiles>
    <profile>
      <id>dev</id>
      <activation>
        <property>
          <name>!release</name>
        </property>
      </activation>
      <properties>
        <package-version>${base-package-version}.dev${build-id}</package-version>
      </properties>
    </profile>
    <profile>
      <id>release</id>
      <activation>
        <property>
          <name>release</name>
        </property>
      </activation>
      <properties>
        <package-version>${base-package-version}</package-version>
      </properties>
    </profile>
  </profiles>
</project>
